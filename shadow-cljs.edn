;; shadow-cljs configuration
{:source-paths
 ["src/"]

 :dependencies
 [[instaparse "1.4.12"]
  [reagent "1.2.0"]]

 :dev-http {8090 "public"}
 :builds
 {:engine {:target :node-library
           :output-to "output.js"
           :exports {:parse bean.parser/parse}}
